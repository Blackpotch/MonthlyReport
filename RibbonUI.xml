<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="initRibbon">
  <ribbon startFromScratch="false">
    <tabs>
      <tab id="tabRosters" label="REPORT">
        <group id="navGroup" label="Navigation">
          <button id="rbtFruitConfiscated" label="Fruit Confiscated" imageMso="WebPagePreview" onAction="GoFruitConfiscated" />
          <button id="rbtCommercialVehicles" label="Commercial Vehicles" imageMso="ReviewShowOrHideMarkup" onAction="GoCommercialVehicles" />
          <button id="rbtCars" label="Cars" imageMso="Breakpoint" onAction="GoCars" />
          <button id="rbtNightShift" label="Night Shift" imageMso="PictureContrastGallery" onAction="GoNightShift" />
          <button id="rbtVehiclesWithFruit" label="Vehicles With Fruit" imageMso="QueryAppend" onAction="GoVehiclesWithFruit" />
          <button id="rbtFoodland" label="Foodland" imageMso="DollarSign" onAction="GoFoodland" />
          <separator id="sepNavMain" />
          <button id="rbtStock" label="Stock" imageMso="Piggy" onAction="GoStock" />
          <button id="rbtPreviousData" label="Previous Data" imageMso="RecoverInviteToMeeting" onAction="GoPreviousData" />
          <button id="rbtSummary" label="Summary" imageMso="PageSetupPageDialog" onAction="GoSummary" />
        </group>
        <group id="settingsGroup" label="Settings">
          <dropDown id="ddwnYears" label="Year Ending:" onAction="ChangeYear" getSelectedItemID="GetYear">
            <item id="id2017" label="2017" />
            <item id="id2018" label="2018" />
            <item id="id2019" label="2019" />
            <item id="id2020" label="2020" />
            <item id="id2021" label="2021" />
          </dropDown>
          <dropDown id="ddwnMonths" label="Month:" onAction="ChangeMonth" getSelectedItemID="GetMonth">
            <item id="id1" label="January" />
            <item id="id2" label="February" />
            <item id="id3" label="March" />
            <item id="id4" label="April" />
            <item id="id5" label="May" />
            <item id="id6" label="June" />
            <item id="id7" label="July" />
            <item id="id8" label="August" />
            <item id="id9" label="September" />
            <item id="id10" label="October" />
            <item id="id11" label="November" />
            <item id="id12" label="December" />
          </dropDown>
          <separator id="sepSettings" />
          <dropDown id="idLocation" label="Location:" onAction="SetLocation">
            <item id="idCEDUNA" label="CEDUNA"></item>
            <item id="idOODLAWIRRA" label="OODLA WIRRA"></item>
            <item id="idPINNAROO" label="PINNAROO"></item>
            <item id="idYAMBA" label="YAMBA"></item>
          </dropDown>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>